#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int x=2;
	int y=5;
	while(t--)
	{

	
	int n,m;
	cin>>n>>m;
	if(x*n>y*m)
	{
	    cout<<"chocolate"<<endl;
	}
	else if(y*m>x*n)
	{
	    cout<<"candy"<<endl;
	}
	else if(y*m==x*n)
	{
	    cout<<"either"<<endl;
	}
	}
	
	return 0;
}
