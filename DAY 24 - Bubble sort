#include <bits/stdc++.h> 
void bubbleSort(vector<int>& arr, int n)
{   
    // Write your code here.
    for(int i=0;i<n-1;i++)
        //for round 1 to n-1
    {
        for(int j=0;j<n-1;j++ )
            //processing elements till n-1
        if(arr[j]>arr[j+1])
            
            swap(arr[j],arr[j+1]);
    }
 
    
}
